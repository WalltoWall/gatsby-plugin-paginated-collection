{
  "name": "gatsby-plugin-paginated-collection",
  "version": "0.3.0-next.1",
  "description": "Gatsby plugin which paginates a collection of data from your sources",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist",
    "gatsby-node.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production npm run clean && rollup -c",
    "format": "prettier --write '{{src,docs}/**/*.{ts,tsx,md}}'",
    "prepare": "npm run build",
    "release": "standard-version",
    "test": "jest"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "pagination"
  ],
  "author": "Angelo Ashmore <angelo.ashmore@walltowall.com>",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "jest": "^25.2.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.1",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.3.0",
    "tsdx": "^0.13.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "standard-version": {
    "tagPrefix": "gatsby-plugin-paginated-collection@"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "proseWrap": "always",
    "printWidth": 80
  }
}
